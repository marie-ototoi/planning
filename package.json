  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marie-ototoi/planning/issues"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "cookie-session": "^2.0.0-alpha.2",
    "csurf": "^1.9.0",
    "d3": "^4.4.3",
    "d3-time-format": "^2.0.3",
    "express": "~4.14.0",
    "express-session": "^1.14.2",
    "mongoose": "^4.7.7",
    "passport": "^0.3.2",
    "passport-twitter": "^1.0.4",
    "pug": "^2.0.0-beta6"
  },
  "homepage": "https://github.com/marie-ototoi/planning#readme",
